<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#FFFFFF">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#DCF4FF" />
            <corners android:radius="35dp" />
            <size
                android:width="120dp"
                android:height="30sp" />
            <padding
                android:bottom="0dp"
                android:left="5dp"
                android:right="5dp"
                android:top="0dp">
            </padding>
        </shape>
    </item>
</ripple>